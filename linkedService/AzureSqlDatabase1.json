{
	"name": "AzureSqlDatabase1",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"Servername": {
				"type": "String",
				"defaultValue": "abc"
			},
			"databasename": {
				"type": "String",
				"defaultValue": "abc"
			},
			"username": {
				"type": "String",
				"defaultValue": "123"
			},
			"password": {
				"type": "String",
				"defaultValue": "123"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=@{linkedService().Servername};Initial Catalog=@{linkedService().databasename};User ID=@{linkedService().username};Password=@{linkedService().username}"
		}
	}
}